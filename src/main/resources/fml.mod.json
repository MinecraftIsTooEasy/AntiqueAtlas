{
	"schemaVersion": 1,
	"id": "antiqueatlas",
	"version": "${version}",
	"name": "Antique Atlas",
	"description": "Antique Atlas is a book that acts like a map featuring infinite scrolling, zoom and custom labeled markers. The map is generated around the player by calculating the average biome in each 16x16 chunk.",
	"authors": [
		"Hunternif",
		"Xy_Luce"
	],
	"contributors": [
		"coolAlias",
		"Kenkron",
		"TheCodeWarrior",
		"CapoFantasma97",
		"Mazdallier"
	],
	"contact": {
		"homepage": "https://minecraftistooeasy.github.io/",
		"sources": "https://github.com/MinecraftIsTooEasy/AntiqueAtlas"
	},
	"license": "GPL-v3",
	"icon": "assets/antiqueatlas/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"hunternif.mc.atlas.AntiqueAtlasMod"
		],
		"registry": [
			"hunternif.mc.atlas.AntiqueAtlasItem"
		]
	},
	"depends": {
		"fishmodloader": ">=3.4.0",
		"minecraft": "1.6.4-mite",
		"java": ">=17",
		"rusted_iron_core": ">=1.3.5"
	},
	"suggests": {
		"another-mod": "*"
	},
	"accessWidener": "antiqueatlas.accesswidener",
	"mixins": [
		"antiqueatlas.mixins.json"
	]
